#include <iostream>
using namespace std;

int main() {
  int i, j;
  cout << "Contoh Program Pengulangan" << endl;
    for (i=1; i<=10; i++)
    cout << i << endl;
  cout << "\n";
  
  cout << "Contoh Program Pengulangan Menurun" << endl;
    for (i=10; i>=1; i--)
    cout << i << endl;
  return 0;
}
